        </policy>
        <policy id="frame-src">
            <values>
                <value id="getivy_de" type="host">*.getivy.de</value>
            </values>
        </policy>
        <policy id="form-action">
            <values>
                <value id="getivy_de" type="host">*.getivy.de</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="getivy_de" type="host">*.getivy.de</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
